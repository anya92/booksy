{
  "name": "booksy",
  "version": "1.0.0",
  "main": "index.js",
  "now": {
    "dotenv": "variables.env"
  },
  "scripts": {
    "start": "node build/bundle.js",
    "dev": "npm-run-all --parallel dev:*",
    "dev:server": "nodemon --watch build --exec node build/bundle.js",
    "dev:build-server": "webpack --config webpack.server.js --watch --mode development",
    "dev:build-client": "webpack --config webpack.client.js --watch --mode development",
    "build": "npm-run-all --parallel prod:*",
    "prod:build-server": "webpack --config webpack.server.js --mode production",
    "prod:build-client": "webpack --config webpack.client.js --mode production"
  },
  "keywords": [
    "react",
    "ssr",
    "graphql",
    "apollo",
    "books"
  ],
  "author": "Ania Brajtkopf",
  "license": "ISC",
  "dependencies": {
    "apollo-cache-inmemory": "^1.1.10",
    "apollo-client": "^2.2.6",
    "apollo-link": "^1.2.1",
    "apollo-link-http": "^1.5.3",
    "apollo-link-ws": "^1.0.7",
    "apollo-server-express": "^2.0.4",
    "apollo-utilities": "^1.0.9",
    "axios": "^0.18.0",
    "connect-mongo": "^2.0.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "css-loader": "^1.0.0",
    "date-fns": "^1.29.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "graphql": "^0.13.2",
    "graphql-subscriptions": "^0.5.8",
    "graphql-tag": "^2.8.0",
    "graphql-tools": "^3.1.1",
    "lodash": "^4.17.10",
    "mongoose": "^5.2.9",
    "node-fetch": "^2.1.1",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "react": "^16.3.2",
    "react-apollo": "^2.0.4",
    "react-dom": "^16.3.2",
    "react-loadable": "^5.5.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "react-toastify": "^4.2.0",
    "recompose": "^0.28.2",
    "serialize-javascript": "^1.4.0",
    "style-loader": "^0.22.1",
    "styled-components": "^3.2.0",
    "webpack": "^4.1.0",
